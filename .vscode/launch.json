    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            // "name": "Launch Program",
            "name": "启动程序",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${file}"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "无环境浏览器联调",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "runtimeExecutable": "node-inspect",
            "program": "${file}"
        }
    ]
}